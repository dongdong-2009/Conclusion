<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog"/>
  </configSections>



  <connectionStrings>
    <add connectionString="Server=155.69.219.62; port=5000;DataBase=nslh_test;user id=root;Password=thisishvac;persist security info=False" name="MySQLDB"/>
  </connectionStrings>
  
  <appSettings>
    <!--The three database tables which this modbus module required -->
    <add key="ModbusDBTable_Group" value="modbus_group"/>
    <add key="ModbusDBTable_StationInfo" value="modbus_stationinfo"/>
    <add key="ModbusDBTable_Distribution" value="modbus_distribution"/>
    <add key="ReadPollSeconds" value="10"/>
    <add key="ClientID" value="1"/>
  </appSettings>
  
 <nlog atuoReload="true">
    <targets>
      <target name="t1" type="File" fileName="${basedir}/logs/${shortdate}.log" layout="[${level}] ${longdate} ${callsite} &gt;&gt; ${message}"/>
      <!-- <target name="t1" type="File" fileName="${basedir}/logs/${shortdate}.log" layout="[${level}] ${longdate} ${callsite} >> ${message}$ {event-context:item=exception} ${stacktrace} ${event-context:item=stacktrace}"/> -->
      <target name="t2" type="Console" layout="${date:format=yyyyMMddHHmmss} ${callsite} ${level} ${message}"/>
    </targets>
    <rules>
      <logger name="*" minlevel="Debug" maxlevel="Error" writeTo="t1,t2"/>
    </rules>
  </nlog>
    
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/></startup></configuration>
